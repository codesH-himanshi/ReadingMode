{
  "manifest_version": 3,
  "name": "Reading Mode",
  "version": "1.0",
  "description": "Turns the page dark and grayscale!",
  "icons": {
    "16": "icons/icon16.png"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"]
}